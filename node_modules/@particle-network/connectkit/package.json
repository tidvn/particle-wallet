{
    "name": "@particle-network/connectkit",
    "version": "1.3.5",
    "description": "Particle Connect React Component",
    "private": false,
    "type": "module",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "module": "esm/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "echo '{ \"type\": \"commonjs\" }' > dist/package.json && node esBuild.js",
        "build:watch": "yarn build --watch",
        "clean": "rm -rf dist && rm -rf esm",
        "dev": "yarn clean && NODE_ENV=development yarn build:watch & NODE_ENV=development yarn typegen:watch",
        "prepare": "MINIFY_CSS=true yarn build",
        "prebuild": "yarn typegen",
        "typecheck": "yarn tsc --noEmit",
        "typegen": "tsc --emitDeclarationOnly || true",
        "typegen:watch": "tsc --emitDeclarationOnly --watch",
        "build:lib": "yarn build",
        "build:min.js": "shx rm -rf dist && webpack --config webpack.min.config.js",
        "test": "jest"
    },
    "directories": {
        "dist": "dist",
        "test": "test"
    },
    "files": [
        "dist",
        "esm"
    ],
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@particle-network/auth-core-modal": "^1.3.5",
        "@particle-network/connectors": "^1.3.2",
        "i18next": "^22.0.6",
        "qrcode": "^1.5.1",
        "react-i18next": "^12.0.0",
        "react-svg": "^16.1.6"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/node": "^7.10.5",
        "@babel/polyfill": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.18.6",
        "@babel/preset-typescript": "^7.1.0",
        "@babel/register": "^7.11.5",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.7.23",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/styled-components": "^5.1.19",
        "babel-eslint": "^10.1.0",
        "compression-webpack-plugin": "^10.0.0",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^6.7.1",
        "esbuild-loader": "^2.20.0",
        "file-loader": "^6.2.0",
        "mini-css-extract-plugin": "^2.6.1",
        "postcss-loader": "^7.0.2",
        "postcss-prefix-selector": "^1.16.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "stream-browserify": "^3.0.0",
        "terser-webpack-plugin": "^5.3.6",
        "traceur-loader": "^0.6.3",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "url-loader": "^4.1.1",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "browserslist": [
        "> 0.1%",
        "last 2 versions",
        "ie>=9"
    ],
    "gitHead": "f69d95e3c914f0e2c7c230ba762507ead7563f68"
}
