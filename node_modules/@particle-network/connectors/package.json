{
    "name": "@particle-network/connectors",
    "version": "1.3.2",
    "license": "Apache-2.0",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./esm/index.js",
    "types": "./dist/index.d.ts",
    "homepage": "https://particle.network",
    "directories": {
        "dist": "dist",
        "test": "test"
    },
    "files": [
        "dist",
        "esm"
    ],
    "engines": {
        "node": ">=16"
    },
    "author": "Particle Network",
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "ethereum",
        "solana",
        "wallet",
        "blockchain",
        "auth",
        "mpc",
        "connect"
    ],
    "scripts": {
        "build": "echo '{ \"type\": \"commonjs\" }' > dist/package.json && node esBuild.js",
        "build:watch": "yarn build --watch",
        "clean": "rm -rf dist && rm -rf esm",
        "dev": "yarn clean && NODE_ENV=development yarn build:watch & NODE_ENV=development yarn typegen:watch",
        "prepare": "MINIFY_CSS=true yarn build",
        "prebuild": "yarn typegen",
        "typecheck": "yarn tsc --noEmit",
        "typegen": "tsc --emitDeclarationOnly || true",
        "typegen:watch": "tsc --emitDeclarationOnly --watch",
        "build:lib": "yarn build",
        "build:min.js": "shx rm -rf dist && webpack --config webpack.min.config.js",
        "test": "jest"
    },
    "dependencies": {
        "@coinbase/wallet-sdk": "^3.7.1",
        "@particle-network/chains": "*",
        "@walletconnect/ethereum-provider": "~2.9.1",
        "@walletconnect/modal": "~2.6.0",
        "eventemitter3": "^4.0.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/core": "^7.11.6",
        "@babel/node": "^7.10.5",
        "@babel/polyfill": "^7.11.5",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.1.0",
        "@babel/register": "^7.11.5",
        "@solana/web3.js": "^1.58.0",
        "babel-eslint": "^10.1.0",
        "compression-webpack-plugin": "^10.0.0",
        "crypto-browserify": "^3.12.0",
        "css-loader": "^6.7.1",
        "file-loader": "^6.2.0",
        "jest": "^28.1.3",
        "stream-browserify": "^3.0.0",
        "terser-webpack-plugin": "^5.3.6",
        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.1",
        "url-loader": "^4.1.1",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "gitHead": "f69d95e3c914f0e2c7c230ba762507ead7563f68"
}
